{
  "name": "cep-koa-api",
  "version": "4.0.0",
  "repository": "lagden/cep-koa-api",
  "description": "Encontre os endereços através do CEP",
  "keywords": [
    "cep",
    "busca",
    "api",
    "koa2"
  ],
  "engines": {
    "node": "8.5.0"
  },
  "dependencies": {
    "debug": "3.1.0",
    "got": "8.0.1",
    "graphql": "0.11.7",
    "ioredis": "3.2.2",
    "koa-app-base": "2.0.0",
    "koa-bodyparser": "4.2.0",
    "koa-router": "7.3.0",
    "lagden-cep": "1.2.0"
  },
  "devDependencies": {
    "ava": "0.24.0",
    "coveralls": "2.13.3",
    "hash-to-port": "1.0.0",
    "nyc": "11.3.0",
    "supertest": "3.0.0",
    "xo": "0.18.2"
  },
  "scripts": {
    "report": "nyc report --reporter=html",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "pretest": "xo",
    "test": "NODE_ENV=test RPASS=surftrip nyc ava",
    "start": "NODE_ENV=dev DEBUG=cep-api:* node index.js"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "semicolon": false,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "capitalized-comments": 0
    }
  },
  "main": "index.js",
  "files": [
    "app",
    "LICENSE",
    "readme.md"
  ],
  "license": "MIT"
}
