{
  "name": "cep-koa-api",
  "version": "7.3.1",
  "repository": "lagden/cep-koa-api",
  "description": "Encontre os endereços através do CEP",
  "keywords": [
    "cep",
    "busca",
    "api"
  ],
  "engines": {
    "node": ">= 12"
  },
  "scripts": {
    "coverage": "nyc --reporter=text-summary --reporter=lcov ava --serial -t",
    "pretest": "xo",
    "test": "ava --serial -t",
    "start": "node server",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "main": "index.js",
  "files": [
    "app",
    "LICENSE",
    "readme.md"
  ],
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "6.2.3",
    "@graphql-tools/load": "6.2.4",
    "@graphql-tools/schema": "6.2.3",
    "@koa/router": "9.4.0",
    "@tadashi/cep": "1.9.1",
    "@tadashi/koa-base": "3.16.0",
    "debug": "4.3.0",
    "graphql": "15.3.0",
    "koa-bodyparser": "4.3.0",
    "koa-compose": "4.1.0",
    "level": "6.0.1",
    "lodash.merge": "4.6.2",
    "snyk": "1.401.0",
    "supports-color": "7.2.0"
  },
  "devDependencies": {
    "@tadashi/hex-id": "2.0.0",
    "ava": "3.12.1",
    "hash-to-port": "1.1.0",
    "nyc": "15.1.0",
    "supertest": "4.0.2",
    "xo": "0.33.1"
  },
  "ava": {
    "files": [
      "test/**/*",
      "!test/helpers/**/*"
    ],
    "timeout": "30s"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "semicolon": false,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "capitalized-comments": 0,
      "padding-line-between-statements": 0,
      "require-atomic-updates": 0
    }
  },
  "license": "MIT",
  "snyk": true
}
