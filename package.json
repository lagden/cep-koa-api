{
  "name": "cep-koa-api",
  "version": "7.0.1",
  "repository": "lagden/cep-koa-api",
  "description": "Encontre os endereços através do CEP",
  "keywords": [
    "cep",
    "busca",
    "api"
  ],
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "report": "nyc report --reporter=text-lcov | coveralls",
    "pretest": "xo",
    "test": "ava --serial -t",
    "start": "nodemon server"
  },
  "main": "index.js",
  "files": [
    "app",
    "LICENSE",
    "readme.md"
  ],
  "dependencies": {
    "@koa/router": "8.0.8",
    "@tadashi/cep": "1.8.0",
    "@tadashi/koa-base": "3.11.0",
    "debug": "4.1.1",
    "graphql": "14.5.8",
    "graphql-tools": "4.0.7",
    "koa-bodyparser": "4.2.1",
    "koa-compose": "4.1.0",
    "level": "6.0.0",
    "lodash.merge": "4.6.2",
    "supports-color": "7.1.0"
  },
  "devDependencies": {
    "@tadashi/hex-id": "1.1.1",
    "ava": "3.0.0",
    "hash-to-port": "1.1.0",
    "nodemon": "2.0.2",
    "supertest": "4.0.2",
    "xo": "0.25.3"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "semicolon": false,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "capitalized-comments": 0,
      "padding-line-between-statements": 0,
      "require-atomic-updates": 0
    }
  },
  "license": "MIT"
}
