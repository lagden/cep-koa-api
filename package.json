{
  "name": "cep-koa-api",
  "version": "8.0.0",
  "repository": "lagden/cep-koa-api",
  "description": "Encontre os endereços através do CEP",
  "keywords": [
    "cep",
    "busca",
    "api"
  ],
  "type": "module",
  "main": "server/index.js",
  "files": [
    "server",
    "README.md"
  ],
  "author": "Thiago Lagden",
  "license": "MIT",
  "scripts": {
    "eslint": "eslint --config .eslintrc.yml --ext .js --ignore-path .gitignore .",
    "prettier": "prettier -c --plugin-search-dir=. ./server/**/*.js",
    "pretest": "npm run eslint",
    "test": "c8 --reporter=text --reporter=text-summary --reporter=lcov ava --serial",
    "dev": "run-s test start",
    "start": "node server",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "7.1.0",
    "@graphql-tools/load": "7.3.2",
    "@graphql-tools/schema": "8.2.0",
    "@koa/router": "10.1.1",
    "@tadashi/cep": "2.0.0",
    "@tadashi/find-in-cache": "2.2.0",
    "@tadashi/koa-base": "5.1.0",
    "debug": "4.3.2",
    "graphql": "15.6.0",
    "ioredis": "4.27.9",
    "koa": "2.13.3",
    "koa-bodyparser": "4.3.0",
    "koa-compose": "4.1.0",
    "local-access": "1.1.0",
    "lodash.merge": "4.6.2",
    "snyk": "1.727.0"
  },
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@babel/eslint-parser": "7.15.7",
    "@babel/plugin-syntax-top-level-await": "7.14.5",
    "@tadashi/hex-id": "2.0.0",
    "ava": "3.15.0",
    "c8": "7.9.0",
    "eslint": "7.32.0",
    "eslint-config-xo": "0.38.0",
    "eslint-plugin-unicorn": "36.0.0",
    "got": "11.8.2",
    "hash-to-port": "1.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.1"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-syntax-top-level-await"
    ]
  },
  "snyk": true
}
