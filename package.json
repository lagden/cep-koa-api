{
  "name": "cep-koa-api",
  "version": "6.3.0",
  "repository": "lagden/cep-koa-api",
  "description": "Encontre os endereços através do CEP",
  "keywords": [
    "cep",
    "busca",
    "api"
  ],
  "engines": {
    "node": ">= 8.9.1"
  },
  "scripts": {
    "report": "nyc report --reporter=text-lcov | coveralls",
    "pretest": "xo",
    "test": "ava --serial -t",
    "start": "nodemon src/index.js"
  },
  "main": "index.js",
  "files": [
    "app",
    "LICENSE",
    "readme.md"
  ],
  "dependencies": {
    "@tadashi/cache-redis": "4.0.0",
    "@tadashi/cep": "1.6.0",
    "debug": "4.1.1",
    "graphql": "14.4.2",
    "graphql-tools": "4.0.5",
    "koa-app-base": "3.8.0",
    "koa-bodyparser": "4.2.1",
    "koa-compose": "4.1.0",
    "koa-router": "7.4.0",
    "lodash.merge": "4.6.2"
  },
  "devDependencies": {
    "ava": "2.2.0",
    "hash-to-port": "1.1.0",
    "nodemon": "1.19.1",
    "supertest": "4.0.2",
    "xo": "0.24.0"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "semicolon": false,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "capitalized-comments": 0,
      "padding-line-between-statements": 0
    }
  },
  "ava": {
    "helpers": [
      "test/helpers/**/*"
    ]
  },
  "license": "MIT"
}
