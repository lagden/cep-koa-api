{
  "name": "cep-koa-api",
  "version": "1.2.0",
  "repository": "lagden/cep-koa-api",
  "description": "Encontre os endereços através de busca por CEP",
  "keywords": [
    "cep",
    "busca",
    "api",
    "koa2"
  ],
  "engines": {
    "node": ">=7.10.0"
  },
  "dependencies": {
    "debug": "2.6.8",
    "got": "7.0.0",
    "ioredis": "3.1.1",
    "kcors": "2.2.1",
    "koa": "2.2.0",
    "koa-compress": "2.0.0",
    "koa-conditional-get": "2.0.0",
    "koa-etag": "3.0.0",
    "koa-response-time": "2.0.0",
    "koa-router": "7.2.0",
    "lagden-cep": "1.1.1"
  },
  "devDependencies": {
    "ava": "0.19.1",
    "coveralls": "2.13.1",
    "nyc": "11.0.2",
    "supertest": "3.0.0",
    "xo": "0.18.2"
  },
  "scripts": {
    "report": "nyc report --reporter=html",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "pretest": "xo",
    "test": "NODE_ENV=test nyc ava --serial",
    "start": "NODE_ENV=develop DEBUG=cep-api:* node index.js"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "semicolon": false,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "capitalized-comments": 0
    }
  },
  "ava": {
    "concurrency": 2,
    "timeout": "30s"
  },
  "main": "index.js",
  "files": [
    "app",
    "LICENSE",
    "readme.md"
  ],
  "license": "MIT"
}
