{
  "name": "cep-koa-api",
  "version": "7.1.0",
  "repository": "lagden/cep-koa-api",
  "description": "Encontre os endereços através do CEP",
  "keywords": [
    "cep",
    "busca",
    "api"
  ],
  "engines": {
    "node": ">= 12"
  },
  "scripts": {
    "report": "nyc report --reporter=text-lcov | coveralls",
    "pretest": "xo",
    "test": "ava --serial -t",
    "start": "nodemon server",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "main": "index.js",
  "files": [
    "app",
    "LICENSE",
    "readme.md"
  ],
  "dependencies": {
    "@koa/router": "8.0.8",
    "@tadashi/cep": "1.9.0",
    "@tadashi/koa-base": "3.11.0",
    "debug": "4.1.1",
    "graphql": "15.0.0",
    "graphql-tools": "5.0.0",
    "koa-bodyparser": "4.3.0",
    "koa-compose": "4.1.0",
    "level": "6.0.1",
    "lodash.merge": "4.6.2",
    "supports-color": "7.1.0",
    "snyk": "^1.317.0"
  },
  "devDependencies": {
    "@tadashi/hex-id": "2.0.0",
    "ava": "3.7.1",
    "hash-to-port": "1.1.0",
    "supertest": "4.0.2",
    "xo": "0.29.1"
  },
  "ava": {
    "files": [
      "test/**/*",
      "!test/helpers/**/*"
    ]
  },
  "xo": {
    "envs": [
      "node"
    ],
    "semicolon": false,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "capitalized-comments": 0,
      "padding-line-between-statements": 0,
      "require-atomic-updates": 0
    }
  },
  "license": "MIT",
  "snyk": true
}
