{
  "name": "cep-koa-api",
  "version": "6.0.0",
  "repository": "lagden/cep-koa-api",
  "description": "Encontre os endereços através do CEP",
  "keywords": [
    "cep",
    "busca",
    "api",
    "koa2"
  ],
  "engines": {
    "node": "8.9.1"
  },
  "scripts": {
    "pretest": "xo",
    "test": "NODE_ENV=test ava --serial",
    "preci": "npm run pretest",
    "ci": "NODE_ENV=test nyc ava --serial",
    "coverage": "NODE_ENV=test nyc report --reporter=text-lcov | coveralls",
    "start": "nodemon index.js"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "semicolon": false,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "capitalized-comments": 0
    }
  },
  "main": "index.js",
  "files": [
    "app",
    "LICENSE",
    "readme.md"
  ],
  "license": "MIT",
  "dependencies": {
    "@tadashi/cache-redis": "1.0.0",
    "@tadashi/cep": "1.2.2",
    "@tadashi/debug": "2.1.0",
    "globby": "8.0.1",
    "graphql": "0.13.2",
    "koa-app-base": "3.1.1",
    "koa-bodyparser": "4.2.0",
    "koa-compose": "4.0.0",
    "koa-router": "7.4.0"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "coveralls": "3.0.0",
    "hash-to-port": "1.0.0",
    "nodemon": "1.17.3",
    "nyc": "11.6.0",
    "supertest": "3.0.0",
    "xo": "0.20.3"
  }
}
